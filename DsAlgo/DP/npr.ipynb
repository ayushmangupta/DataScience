    "def permutation(n,k):\n",
    "    fact = [0]*(n+1)\n",
    "    fact[0] =1\n",
    "    for i in range(1,n+1):\n",
    "        fact[i] = fact[i-1]*i\n",
    "    return (fact[n]/fact[n-k])"
   ]
